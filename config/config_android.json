    "options": {
        "showResults": true,
        "applyBlur": true,
        "applyLightVariation": false,
        "saveImagesWithResults": false,
        "enableCLAHE": false,
        "enablePreprocessing": false,
        "imageInferenceMode": true
    },
    "ppeDetectionClasses": [
        "Hardhat",
        "NO-Hardhat",
        "Safety Vest",
        "NO-Safety Vest"
    ]
