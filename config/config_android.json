{
    "paths": {
        "model": "./models/best_saved_model/best_float32.tflite",
        "results": "./runs",
        "input": "./assets/ppe-3.mp4",
        "inputImagesPath": "./data/images/",
        "outputLabelsPath": "./data/labels/predictions"
    },
    "settings": {
        "lightVariationChance": 0.1,
