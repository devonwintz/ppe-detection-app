    "options": {
        "showResults": true,
        "applyBlur": true,
        "applyLightVariation": true,
        "saveImagesWithResults": false,
        "enableCLAHE": false,
        "enablePreprocessing": true
    },
    "ppeDetectionClasses": [
        "Hardhat",
        "NO-Hardhat",
        "Mask",
        "NO-Mask",
        "Safety Vest",
        "NO-Safety Vest"
    ]
}