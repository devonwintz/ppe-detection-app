{
    "paths": {
        "model": "./models/best.pt",
        "results": "./runs",
        "input": "./assets/ppe-3.mp4"
    },
    "settings": {
        "lightVariationChance": 0.1,
        "blurChance": 0.1,
        "inputSize": 640,
        "frameSkip": 5,
        "resolution": [
            640,
            480
        ],
        "frameDelayMilliseconds": 100,
        "confidenceThreshold": 0.5
